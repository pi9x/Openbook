{
    "version": "2.0",
    "extensions": {
        "authentication": {
            "jwtBearer": {
                "audiences": [ "your-audience" ],
                "issuer": "your-issuer",
                "validationParameters": {
                    "ValidateIssuer": false,
                    "ValidateAudience": false,
                    "ValidateLifetime": true,
                    "ValidateIssuerSigningKey": false
                }
            }
        }
    },
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            }
        }
    }
}